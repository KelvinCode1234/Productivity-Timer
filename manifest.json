{
  "manifest_version": 3,
  "name": "Productivity Timer",
  "version": "1.0",
  "description": "A simple productivity timer extension to help you stay focused.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon48.png"
  },
  "permissions": ["notifications", "storage"],
  "host_permissions": ["*://*/*"]
}






